---
import strings from '../../strings.mjs';
import MDXWrapper from '../components/MDXWrapper.astro';
import Content from '../content/pages/index.mdx';
import Layout from '../layouts/Layout.astro';
---

<Layout title={strings.title} description={strings.description} permalink={Astro.url.toString()}>
    <header id="hero" aria-label="Hero" role="banner" class="-mx-8 h-[70vh] select-none">
        <script type="module" src="https://unpkg.com/@splinetool/viewer@1.9.46/build/spline-viewer.js" async></script>
        <spline-viewer
            loading-anim-type="spinner-small-light"
            loading="eager"
            url="https://prod.spline.design/2lSZ2jo5XSOpQl8z/scene.splinecode"
            ><img
                src="data:image/png;base64,"
                alt="Spline preview"
                style="width: 100%; height: 100%;"
            /></spline-viewer
        >
    </header>

    <div class="mdx-content mx-auto max-w-screen-lg">
        <MDXWrapper>
            <Content />
        </MDXWrapper>
    </div>
</Layout>
